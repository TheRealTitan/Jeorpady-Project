<Window x:Class="Jeorpady_Project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Jeorpady_Project"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        KeyDown="EscapeWindowHandler">
    <Grid  Name="mainGrid" Background="Black" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
        <Frame x:Name="Main" NavigationUIVisibility="Hidden" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0,0,0,0"/>
    </Grid>
</Window>
